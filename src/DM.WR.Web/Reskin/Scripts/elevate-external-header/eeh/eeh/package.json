        "build:ci": "CI=false react-app-rewired build",
        "format": "prettier --write 'src/**/*.{js,ts,jsx,tsx,json,css,scss}' 'public/**/*.{css,scss,html}'",
        "test": "react-app-rewired test",
        "eject": "react-app-rewired eject"
