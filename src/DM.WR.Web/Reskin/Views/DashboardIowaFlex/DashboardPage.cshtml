@model DashboardIowaFlexViewModel
@{
    if (Model.ContentIndicator == "pdf")
    {
        Layout = MVC.Shared.Views._Layout_Pdf;
    }
    else if (Model.IsDemo)
    {
        Layout = MVC.Shared.Views.dm_ui._Layout_NoMenu;
    }
    ViewBag.Title = "IowaFlex Dashboard";
    ViewBag.MainTitle = "IowaFlex Dashboard";
}

@if (Model.ContentIndicator == "pdf")
{
    ViewBag.pdf_wrapper = "pdf-wrapper dashboard-main-page";
    <div id="pdf-debug-console"></div>
}

<div>
    <img src="@Links.Reskin.Content.img.logoIowaAssessmentsAdaptive_png" class="logoIowaAssessmentsAdaptive dashboard-page" />
    <div class="section-card filters root-tab-element" @*tabindex="1" data-tabindex="1"*@ role="region" aria-label="Dashboard Filters">
        <div class="root-tab-hidden-content" @*aria-hidden="true"*@></div>
    </div>

    @Html.HiddenFor(m => m.IsAdaptive)
    <div class="dm-ui-page-container dm-ui-page-container-800 @ViewBag.pdf_wrapper">
        <section class="dm-ui-page-section carded-section dashboard-page-section">
            <div class="section-card root-tab-element" @*tabindex="2" data-tabindex="2"*@ role="region" aria-label="Group Average Card">
                <div class="root-tab-hidden-content" @*aria-hidden="true"*@>
                    @*<div class="nvda-root-announce"></div>*@
                    @*<label role="tab" class="nvda-announce" tabindex="-1" tabindex-important="true"></label>*@
                    <h2 id="test-event-subject-name" class="tab first-tab-element" @*tabindex="-1"*@>&nbsp;</h2>
                    <br />
                    <div class="average-scores-wrapper">
                        <div id="average-standard-score"></div>
                        <div class="test-event-text" @*aria-hidden="true"*@>Average Standard Score</div>
                        <div id="pie-chart-score"></div>
                        <div class="test-event-text" @*aria-hidden="true"*@>
                            National Percentile<br />
                            Rank (NPR) of the<br />
                            Average Standard Score
                        </div>
                        <div class="buttons-wrapper">
                            <a class="dm-ui-button-primary dm-ui-button-small" id="longitudinal-button" href="@Model.AppPath/dashboard/iowaflex/longitudinal" style="display: none;" @*tabindex="-1"*@ role="button">Longitudinal Reporting</a>
                            <div class="dm-ui-button-primary dm-ui-button-small tab" id="achievement-ability" style="display:none;" tabindex="0" role="button">Show Achievement / Ability</div>
                        </div>
                    </div>
                    <div class="quantiles-wrapper" role="region" aria-label="Percent of Students in each Performance Band">
                        <span id="total-students-num-quartiles" aria-hidden="true">@*Total Number: <span>0</span>*@</span>
                        <h3 class="quantiles-header-text">Percent of Students in each Performance Band</h3>
                        <div id="barChart4">
                            <span class="sr-only">Activating these buttons will cause content on the page to be updated.</span>
                            @*<div class="dm-ui-overlay-spinner-container-not-hide placed-in-block" style="display: block !important;"><div class="dm-ui-spinner" role="alert" aria-live="assertive"><div></div><div></div><div></div><div></div><p class="accessible-message-placeholder js-loading-spinner-copy">Content is loading...</p></div></div>*@
                            <div class="spinner shifted"><div class="rect1"></div><div class="rect2"></div><div class="rect3"></div><div class="rect4"></div><div class="rect5"></div></div>
                        </div>
                        <div id="quantiles-dashed-line-legend" aria-hidden="true">
                            <span>--</span> Percent nationally in each performance band
                            <div id="chart4bars-debug-warning" class="debug-warning quartile"></div>
                        </div>
                    </div>
                </div>
            </div>

            @{
                var dropdownScores = new List<DropdownItem>
                {
                    new DropdownItem {Text = "Age Percentile Rank", Value = "APR", Selected = true},
                    new DropdownItem {Text = "Grade Percentile Rank", Value = "GPR"},
                    new DropdownItem {Text = "Standard Age Score", Value = "SAS"},
                    new DropdownItem {Text = "Universal Scale Score", Value = "USS"},
                    new DropdownItem {Text = "Age Stanine", Value = "AS"},
                    new DropdownItem {Text = "Grade Stanine", Value = "GS"},
                    new DropdownItem {Text = "Raw Score", Value = "RS"}
                    //new DropdownItem {Text = "Local Stanine", Value = "LS"},
                    //new DropdownItem {Text = "Local Percentile Rank", Value = "LPR"}
                };
                var dropdownContentScope = new List<DropdownItem>
                {
                    new DropdownItem {Text = "Verbal", Value = "V", Selected = true},
                    new DropdownItem {Text = "Quantitative", Value = "Q", Selected = true},
                    new DropdownItem {Text = "Non Verbal", Value = "N", Selected = true},
                    new DropdownItem {Text = "Composite VQ", Value = "VQ", Selected = true},
                    new DropdownItem {Text = "Composite VN", Value = "VN", Selected = true},
                    new DropdownItem {Text = "Composite QN", Value = "QN", Selected = true},
                    new DropdownItem {Text = "Composite VQN", Value = "VQN", Selected = true}
                };
            }

            <div class="section-card roster-table-card root-tab-element" @*tabindex="4" data-tabindex="4"*@ role="region" aria-label="Comparison Table">
                <div class="root-tab-hidden-content" @*aria-hidden="true"*@>
                    @*<label role="tab" class="nvda-announce" tabindex="-1" tabindex-important="true"></label>*@
                    @*<div class="nvda-root-announce"></div>*@
                    <div id="roster-top-info-wrapper" class="hidden">
                        <div id="roster-name-wrapper">
                            <h2 id="roster-name" class="tab first-tab-element" @*tabindex="-1"*@></h2>
                            <div id="roster-switcher-wrapper" class="hidden">
                                <div class="dm-ui-switcher-blue dm-ui-switcher noselect">
                                    <input id="roster-switcher-1" name="roster-switcher" class="roster-switcher" value="Percent" type="radio" checked="checked">
                                    <label for="roster-switcher-1">Percent</label>
                                    <input id="roster-switcher-2" name="roster-switcher" class="roster-switcher" value="Number" type="radio">
                                    <label for="roster-switcher-2">Number</label>
                                </div>
                            </div>
                        </div>
                        <div id="wcag-autocomplete-search-instructions" style="display: none">Begin typing to search, use arrow keys to navigate, Enter to select</div>
                        <div id="wcag-autocomplete-search-results" aria-live="assertive" class="sr-only"></div>
                        <div id="roster-top-buttons-wrapper">
                            <span id="roster-search-field">
                                <span data-field="node_name" class="k-filtercell" data-role="filtercell"><span><span tabindex="0" role="presentation" style="" class="k-widget k-autocomplete k-header k-autocomplete-clearable k-state-default"><input data-bind="value: value" aria-label="Search" data-role="autocomplete" data-text-field="node_name" data-filter="startswith" data-delay="200" data-min-length="1" data-value-primitive="true" type="text" class="k-input tab" autocomplete="off" role="textbox" aria-haspopup="true" aria-disabled="false" aria-readonly="false" aria-autocomplete="list" aria-owns="" placeholder="Enter a Student Name" data-full-title="Student Name" tabindex="0"><span unselectable="on" class="k-icon k-clear-value k-i-close k-hidden" title="Clear" role="button" tabindex="0"></span><span class="k-icon k-i-loading" style="display:none"></span></span><span class="k-widget k-dropdown k-header k-dropdown-operator tab" unselectable="on" role="listbox" aria-haspopup="true" aria-expanded="false" tabindex="0" aria-owns="" aria-live="polite" aria-disabled="false" aria-busy="false" aria-activedescendant="880f3c87-1d90-4c62-bba7-8aa5a7a4b01e"><span unselectable="on" class="k-dropdown-wrap k-state-default"><span unselectable="on" class="k-input">Contains</span><span unselectable="on" class="k-select" aria-label="select"><span class="k-icon k-i-filter"></span></span></span><input class="k-dropdown-operator tab" data-bind="value: operator" data-role="dropdownlist" style="display: none;" aria-label="Contains" tabindex="0"></span><button type="button" class="k-button k-button-icon" title="Clear" aria-label="Clear" data-bind="visible:operatorVisible" style="display: none;"><span class="k-icon k-i-filter-clear"></span></button></span></span>
                            </span>
                            @*@Html.DmUiMultiSelectDropdown(dropdownScores, "Score_Control", "CogAT score filter", false, true, 1, 1, "Placeholder text", false)*@
                            @Html.DmUiSingleSelectDropdown(dropdownScores, "Score_Control", false, "Select score", "CogAT score filter")
                            @Html.DmUiMultiSelectDropdown(dropdownContentScope, "Scope_Control", "CogAT content scope filter", true, true, 1, 7, "Placeholder text", false)
                            <span id="roster-filters-popup-button" class="tab no-spacebar-scrolling" aria-label="Select Report Criteria" role="button"></span>
                            <span id="roster-reset-button" class="dm-ui-button-primary dm-ui-button-small tab no-spacebar-scrolling" title="Reset" role="button"></span>
                        </div>
                        <div id="roster-selected-quantile-label"></div>
                        <div id="roster-selected-domain-label"></div>
                        <div id="roster-selected-cogat-label">
                            <div class="arrow"></div><i>Identified <b id="roster-cogat-label-roster-type">students</b> by<br><span><b>Achievement</b> <b id="roster-cogat-label-achievement-value"></b> and <b>Ability</b> <b id="roster-cogat-label-ability-value"></b> for <b id="roster-cogat-label-scope-value">VQN</b> and <b id="roster-cogat-label-score-value">APR</b></span></i><div class="close tab" tabindex="0" aria-label="Clear Achievement/Ability filter" role="button">X</div>
                        </div>
                    </div>
                    <span id="total-students-num-roster" aria-hidden="true">@*Total Number: <span>0</span>*@</span>
                    @if (Model.ContentIndicator == "web")
                    {
                        <div id="roster-table-wrapper"></div>
                    }
                    <div id="roster-legend-wrapper" class="tab" @*tabindex="-1"*@></div>
                    <div id="roster-debug-warning" class="debug-warning roster"></div>
                </div>
            </div>

            <div class="section-card pld-stage-info-card root-tab-element" @*tabindex="4" data-tabindex="4"*@ role="region" aria-label="PLD Stage Narrative" style="display: none;">
                <div class="root-tab-hidden-content" @*aria-hidden="true"*@>
                    <div class="left-column">
                        <h2 class="tab first-tab-element">Pre-Emerging</h2>
                        <p class="tab last-tab-element">A typical <b>Pre-Emerging</b> has not yet demonstrated a developing mastery of the core mathematical standards and practices of the Kindergarten level. The student can solve some addition and subtraction problems and may demonstrate an emerging concept of place value. The student can classify objects and may be able identify and analyze basic geometric shapes.</p>
                    </div>
                    <div class="right-column">
                        <img src="~/Reskin/Content/img/stage-info-card1.png" />
                    </div>
                </div>
            </div>

            <div class="section-card pld-level-info-card root-tab-element" @*tabindex="4" data-tabindex="4"*@ role="region" aria-label="PLD Level Narrative" style="display: none;">
                <div class="root-tab-hidden-content" @*aria-hidden="true"*@>
                    <div class="left-column tab first-tab-element">
                        <h2>I CAN</h2>
                        <ul>
                            <li>show and name upper- and lowercase letters (RF.K.1)</li>
                            <li>hear and say rhyming words. (RF.K.2)</li>
                            <li>say the sounds in words. (RK.F.3)</li>
                            <li>figure out what words mean (L.K.4)</li>
                            <li>use action words correctly (L.K.1)</li>
                            <li>add /s/ or /es/ to show more than one (L.K.1)</li>
                            <li>add words to a sentence to make it clearer (L.K.1)</li>
                            <li>change sounds to make new words. (RK.K.2)</li>
                            <li>look at words and see how they are the same or different (RF.K.3)</li>
                        </ul>
                    </div>
                    <div class="right-column tab last-tab-element">
                        <h2>I AM READY TO</h2>
                        <ul>
                            <li>read high frequency words (RF.1.3)</li>
                            <li>read first grade site words (RF.1.3)</li>
                        </ul>
                    </div>
                </div>
            </div>
        </section>
        <aside class="dm-ui-page-aside">
