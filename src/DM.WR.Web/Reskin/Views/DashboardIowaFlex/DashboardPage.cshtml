    <div id="modal-dashboard-report-criteria" class="dm-ui-modal-container">
        <div class="dm-ui-modal dm-ui-modal-green" role="dialog" aria-modal="true" aria-label="Select Report Criteria">
            <button type="button" class="close_icon first-tab-element wcag-modal-first-element" aria-label="Close"></button>
            <h1 class="modal-header">Select Report Criteria</h1>
            <section>
                @*<div class="session-modal-text-1 tab" tabindex="0">Enter information below to add a student. Fields with an asterisk are required.</div>*@
                <div class="dm-ui-alert dm-ui-alert-error hidden" role="alert"><a href="#" class="dm-ui-alert-close" aria-label="Close alert message" role="button">×</a>There are no items that match your query. Please change your selections and click Run Report again.</div>
                @{
                    var dropdownScore = new List<DropdownItem>
                    {
                        new DropdownItem {Text = "National Percentile Rank", Value = "NationalPercentileRank", Selected = true},
                        new DropdownItem {Text = "Standard Score", Value = "StandardScore"}
                    };
                    var dropdownBoolean = new List<DropdownItem>
                    {
                        new DropdownItem {Text = "AND", Value = "and"},
                        new DropdownItem {Text = "OR", Value = "or"}
                    };
                    var dropdownScoreRange = new List<DropdownItem>
                    {
                        new DropdownItem {Text = "Low", Value = "1"},
                        new DropdownItem {Text = "Mid", Value = "2"},
                        new DropdownItem {Text = "High", Value = "3"}
                    };
                }
                <table class="table-report-filters" role="presentation">
                    <tr>
                        <td class="column-score">
                            @Html.DmUiSingleSelectDropdown(dropdownScore, "dropdown-score-1", false, "Select score", "SCORE")
                        </td>
                        <td class="column-content-area">
                            @Html.DmUiSingleSelectDropdown(new List<DropdownItem>(), "dropdown-content-area-1", false, "Select content area", "CONTENT AREA", htmlAttributes: new { disabled = "disabled" })
                        </td>
                        <td class="column-condition">
                            @Html.DmUiSingleSelectDropdown(new List<DropdownItem>(), "dropdown-condition-1", false, "Select condition", "CONDITION")
                        </td>
                        <td class="column-val1">
                            <div id="wrapper-input-score-1">
                                <label for="input-score-1-val1">VALUE</label>
                                <input id="input-score-1-val1" type="text" maxlength="3">
                            </div>
                            @*<div id="wrapper-dropdown-score-1" style="display: none;">
                                @Html.DmUiSingleSelectDropdown(dropdownScoreRange, "dropdown-score-range-1", false, "---", "SCORE", htmlAttributes: new { disabled = "disabled" })
                            </div>*@
                        </td>
