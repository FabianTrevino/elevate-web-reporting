{
    "name": "elevate-external-header",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "set HTTPS=true& set PORT=3001& react-app-rewired start --preserve-symlinks",
        "start-local": "set HTTPS=true& set REACT_APP_ELEVATE_ENV=local& set PORT=3001& react-app-rewired start --preserve-symlinks",
        "start-bash": "HTTPS=true REACT_APP_ELEVATE_ENV=local PORT=3001 react-app-rewired start --preserve-symlinks",
        "build": "set HTTPS=true& react-app-rewired build && node ./post-build.js",
